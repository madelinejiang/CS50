#include <stdio.h>
#include <cs50.h>
#include <math.h>

int main(void)
{
    int height;
    int n;
    int spaces;
    int hashtag;

    while(true)
    {

    printf("Height?: ");

    height=get_int();

    if(height<23)
        break;

    else
        {
            printf("Height must be less than 23. Enter in a new number\n");
            continue;
        }

    }


    for( int i=0; i<height; i++)
    {
        spaces=spaces-1;

        printf(" ");

    }


}